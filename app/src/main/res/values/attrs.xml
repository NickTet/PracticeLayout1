<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DrawableTopLeftTextview">
        <attr name="drawableTopLeftLeft" format="reference"/>
        <attr name="drawableTopLeftPadding" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RecordView">
        <attr name="OutRoundColor" format="color"/>
        <attr name="OutRoundWidth" format="integer"/>
        <attr name="InnerRoundColor" format="color"/>
        <attr name="RadiusWidth" format="integer"/>
        <attr name="ProgressCircleWidth" format="integer"/>
        <attr name="ProgressCircleColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="OldThumbUpView">
        <attr name="o_tuv_count" format="integer"/>
    </declare-styleable>
</resources>